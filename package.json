{
  "name": "dev-tools",
  "description": "Useful dev tools accessed via a browser and used like a terminal",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=18.12",
    "npm": ">=8.19"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/liam-hales/dev-tools.git"
  },
  "packageManager": "yarn@3.2.4",
  "scripts": {
    "start:dev": "next dev",
    "start:prod": "next start",
    "build": "next build",
    "build:analyse": "ANALYSE_BUNDLE=true next build",
    "lint": "next lint",
    "deps": "ncu --packageManager yarn",
    "cleanup": "rm -rf .next/"
  },
  "dependencies": {
    "autoprefixer": "10.4.15",
    "camelcase-keys": "^9.0.0",
    "nanoid": "^4.0.2",
    "next": "13.4.19",
    "param-case": "^3.0.4",
    "postcss": "8.4.29",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.3",
    "yargs-parser": "^21.1.1",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^13.4.19",
    "@types/node": "^20.5.7",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/yargs-parser": "^21.0.0",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "eslint": "^8.48.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-next": "^13.4.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "npm-check-updates": "^16.13.2",
    "typescript": "^5.2.2"
  }
}
