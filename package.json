{
  "name": "dev-tools",
  "description": "Useful dev tools accessed via a browser and used like a terminal",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=18.12",
    "npm": ">=8.19"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/liam-hales/dev-tools.git"
  },
  "packageManager": "yarn@4.1.1",
  "scripts": {
    "start:dev": "next dev",
    "start:prod": "next start",
    "build": "next build",
    "build:analyse": "ANALYSE_BUNDLE=true next build",
    "lint": "next lint",
    "deps": "ncu --packageManager yarn",
    "cleanup": "rm -rf .next/"
  },
  "dependencies": {
    "autoprefixer": "10.4.19",
    "camelcase-keys": "^9.1.3",
    "nanoid": "^5.0.7",
    "next": "14.2.2",
    "param-case": "^3.0.4",
    "postcss": "8.4.38",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.4.3",
    "yargs-parser": "^21.1.1",
    "zod": "^3.22.5"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.2.2",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@types/yargs-parser": "^21.0.3",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-next": "^14.2.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "npm-check-updates": "^16.14.18",
    "typescript": "^5.4.5"
  }
}
