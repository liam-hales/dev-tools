{
  "name": "dev-tools",
  "description": "Useful dev tools accessed via a browser and used like a terminal",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=18.12",
    "npm": ">=8.19"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/liam-hales/dev-tools.git"
  },
  "packageManager": "yarn@3.2.4",
  "scripts": {
    "start:dev": "next dev",
    "start:prod": "next start",
    "build": "next build",
    "build:analyse": "ANALYSE_BUNDLE=true next build",
    "lint": "next lint",
    "deps": "ncu --packageManager yarn",
    "cleanup": "rm -rf .next/"
  },
  "dependencies": {
    "autoprefixer": "10.4.14",
    "next": "13.4.1",
    "postcss": "8.4.23",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.2"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^13.4.1",
    "@types/node": "^18.16.5",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "eslint": "^8.40.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "^13.4.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "npm-check-updates": "^16.10.10",
    "typescript": "^5.0.4"
  }
}
